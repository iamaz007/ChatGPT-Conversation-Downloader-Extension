{
  "manifest_version": 2,
  "name": "ChatGPT conversation downloader",
  "description": "Download conversation from ChatGPT in docx file.",
  "version": "0.0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "js": [
        "plugins/jquery-3.6.3.min.js",
        "index.js"
      ]
    }
  ]
}